{
    "name": "TTEST_ONESAMPLE",
    "desc": "Student’s One-sample t-test",
    "label": "T-Test One-Sample",
    "enabled": true,
    "inputdata": {
        "y": {
            "label": "Variables",
            "desc": "Variables",
            "types": [
               "real", "int"
            ],
            "stattypes": [
                "numerical"
            ],
            "notblank": true,
            "multiple": false
        }
    },
    "parameters": {
        "alt_hypothesis": {
            "label": "Alternative Hypothesis",
            "desc": "The alternative hypothesis to the null, returning specifically whether the result is less than, greater than, or .",
            "type": "text",
            "default": "two.sided",
            "enums": ["two.sided", "less", "greater"],
            "notblank": true,
            "multiple": false
        },
        "alpha": {
            "label": "Confidence level",
            "desc": "The confidence level α used in the calculation of the confidence intervals for the correlation coefficients.",
            "type": "real",
            "default": 0.95,
            "min": 0.0,
            "max": 1.0,
            "notblank": true,
            "multiple": false
        },
        "mu": {
            "label": "Population mean",
            "desc": "The population mean, if it is known, else it defaults to 0.",
            "type": "real",
            "default": 0.0,
            "min": -10,
            "max": 10,
            "notblank": true,
            "multiple": false
        }
    },
    "flags": {
        "formula": false
    }
}
